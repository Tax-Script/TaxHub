local function directChineseTranslation()
    local function translateObject(obj)
        if not obj:IsA("TextLabel") and not obj:IsA("TextButton") and not obj:IsA("TextBox") then
            return
        end
        
        local text = obj.Text
        if not text or text == "" then return end
        
        if text == "DYHUB | SK+" then obj.Text = "DYHUB | SK+"
        elseif text:find("Member Count: ") then obj.Text = text:gsub("Member Count: ", "成员数量: ")
        elseif text:find("Online Count: ") then obj.Text = text:gsub("Online Count: ", "在线数量: ")
